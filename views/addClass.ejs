    <hr>    
    <h1>Add a Class</h1>
    <form action="/classes/add" method="post">
        <label>Subject : </label><br>
        <input type="text" name="name_class"><br><br>
        <label>Quota : </label><br>
        <input type="number" name="quota"><br><br>
        <label>Level : </label><br>
        <input type="number" name="level"><br><br>
        <label>Instructor :</label>
        <select name="InstructorId">
            <option></option>
            <% listInstructor.forEach(el => { %>
                <option value="<%= el.id %>"> <%= el.name %>  </option>                
            <% }); %> 
        </select><br><br>  
        <button type="submit">Add Class</button> 
    </form>
