    </ul>

    <table class="table">
        <thead>
          <tr>
            <th scope="col">id</th>
            <th scope="col">name</th>
            <th scope="col">email</th>
            <th scope="col">level</th>
            <th scope="col">total classes</th>
        
          </tr>
        </thead>
        <tbody>
            <% students.forEach(student=> {%>
          <tr>
            <th scope="row"><%=student.id%></th>
            <td><%=student.name%></td>
            <td><%=student.email%></td>
            <td><%=student.faceMaker()%></td>
            <td><%=student.Classes.length%></td>
          </tr>
          <%})%>
        </tbody>
      </table>
